<?xml version='1.0' encoding='utf-8'?>
<widget id="PuntosTeleVIP" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Puntos TeleVIP</name>
    <description>
        Puntos TeleVIP
    </description>
    <author email="a@brickloyalty.mx" href="https://brickloyalty.mx/">
        brickloyalty.com
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://0.0.0.0:8086/" />
    <allow-navigation href="http://0.0.0.0:8086/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon platform="android" qualifier="ldpi" src="KB.png" />
    <icon platform="android" qualifier="mdpi" src="KB.png" />
    <icon platform="android" qualifier="hdpi" src="KB.png" />
    <icon platform="android" qualifier="xhdpi" src="KB.png" />
    <icon platform="android" qualifier="xxhdpi" src="KB.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="KB.png" />
</widget>
